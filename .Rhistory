width = 5,
height = 3,
text = "Inhaltliche Klassifizierung mit LDA-Topic-Modellierung"
),
DiagrammeR::Flowframe(
x = 15,
y = 1,
width = 5,
height = 3,
text = "Weitere Analyse auf thematischer Ebene"
),
DiagrammeR::Flowframe(
x = 1,
y = -2,
